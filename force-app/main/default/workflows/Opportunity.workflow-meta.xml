    <rules>
        <fullName>MSOP 7 Day Alert %28Opp%29</fullName>
        <active>true</active>
        <description>Sends tasks to owner of METRO opp if 7 days after handover MSOP is empty</description>
        <formula>AND(        NOT(ISBLANK(METROOpportunityID__c)),    NOT(ISBLANK(DateOfHandover__c)),    TODAY() - DateOfHandover__c &lt;=8,     ISPICKVAL (  MSOP__c, &quot;&quot;) )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>MSOP_still_open</name>
                <type>Task</type>
            </actions>
            <offsetFromField>Opportunity.DateOfHandover__c</offsetFromField>
            <timeLength>7</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Metro Opp Status Update Auto Accept</fullName>
        <active>true</active>
        <criteriaItems>
            <field>Opportunity.METROOppStatus__c</field>
            <operation>equals</operation>
            <value>Opportunity To be accepted</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.METROOppStatus__c</field>
            <operation>notEqual</operation>
            <value>rejected,In_Progress,Closed Won,Closed Lost,Existing Customer</value>
        </criteriaItems>
        <description>After the 14th days after hand over the Opp gets automatically accepted if the METRO Opp status is still &quot;to be accepted&quot;, see https://jira.orderbird.com/browse/SFDC-1310</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Opp_Metro_Opp_Status_Auto_Accepted</name>
                <type>FieldUpdate</type>
            </actions>
            <offsetFromField>Opportunity.DateOfHandover__c</offsetFromField>
            <timeLength>15</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Metro Opp Status Update Closed Lost</fullName>
        <actions>
            <name>Opp_Metro_Opp_Status_Closed_Lost</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>After the 14th days after hand over the METRO Opp gets set to Closed Lost, in case the Opp is lost, see https://jira.orderbird.com/browse/SFDC-1310</description>
        <formula>AND( 
ISPICKVAL(StageName, &quot;Closed Lost&quot;), 
OR( 
ISPICKVAL( METROOppStatus__c , &quot;Opportunity To be accepted&quot;), 
ISPICKVAL( METROOppStatus__c , &quot;In_Progress&quot;), 
ISPICKVAL( METROOppStatus__c , &quot;Opportunity auto accepted&quot;)) 
)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Metro Opp Status Update Closed Won</fullName>
        <actions>
            <name>Opp_Metro_Opp_Status_Closed_Won</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>After the 14th days after hand over the METRO Opp gets set to Closed Won, in case the Opp is won, see https://jira.orderbird.com/browse/SFDC-1310</description>
        <formula>AND( 
ISPICKVAL(StageName, &quot;Closed Won&quot;), 
OR( 
ISPICKVAL( METROOppStatus__c , &quot;Opportunity To be accepted&quot;), 
ISPICKVAL( METROOppStatus__c , &quot;In_Progress&quot;), 
ISPICKVAL( METROOppStatus__c , &quot;Opportunity auto accepted&quot;)) 
)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Metro Opp Status Update In Progress</fullName>
        <active>true</active>
        <description>If within 14 days after hand over the MSOPs get sets the all-clear, the METRO Opp gets set to In Progress, see https://jira.orderbird.com/browse/SFDC-1310</description>
        <formula>AND( 
OR( 
ISPICKVAL( METROOppStatus__c , &quot;Opportunity To be Accepted&quot;), 
ISPICKVAL( METROOppStatus__c , &quot;Opportunity Auto Accepted&quot;),
ISPICKVAL( METROOppStatus__c , &quot;To be accepted&quot;)), 
ISPICKVAL ( MSOP__c , &quot;All-Clear&quot;) 
)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Opp_Metro_Opp_Status_Accepted_Progr</name>
                <type>FieldUpdate</type>
            </actions>
            <timeLength>1</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Metro Opp Status Update Rejected</fullName>
        <actions>
            <name>Opportunity_Metro_Status_Rejected</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>If within 14 days after hand over the MSOP fails, the METRO Opp Status gets set to &quot;Rejected&quot;, see https://jira.orderbird.com/browse/SFDC-1310</description>
        <formula>AND(     
TODAY() - DateOfHandover__c &lt;= 14,      
OR(        
ISPICKVAL( METROOppStatus__c , &quot;Opportunity To be accepted&quot;),        ISPICKVAL( METROOppStatus__c , &quot;In_Progress&quot;)),        
AND (          
NOT(ISPICKVAL ( MSOP__c, &quot;All-Clear&quot;)),          NOT(ISBLANK(TEXT( MSOP__c))))       
)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
