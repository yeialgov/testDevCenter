    </rules>
    <rules>
        <fullName>Account VAT Logic SImple</fullName>
        <actions>
            <name>Account_Tax_Exempted_True</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND( OR( ISNEW(), ISCHANGED( Tax_Exempt_Reason__c )), OR( ISPICKVAL( Tax_Exempt_Reason__c , &quot;EU reverse charge mechanism&quot;), ISPICKVAL(Tax_Exempt_Reason__c , &quot;Third country jurisdictions&quot;) ) )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
