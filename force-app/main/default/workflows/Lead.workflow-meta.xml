    <rules>
        <fullName>Date of Handover TODAY</fullName>
        <actions>
            <name>Date_of_Handover_TODAY</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>If Referral Contact or Referral Account on Lead get set, but not Date of Handover, the latter automatically gets set to today</description>
        <formula>AND(
 OR(
   NOT(ISBLANK(ReferralAccount__c)),
   NOT(ISBLANK(ReferralContact__c))
 ),
 ISBLANK(DateOfHandover__c)
)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Email New Lead MINI</fullName>
        <actions>
            <name>New_MINI_Lead</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <description>https://jira.orderbird.com/browse/CDS-2398</description>
        <formula>Or ( 
LAC_ID__c = &apos;130&apos;,
LAC_ID__c = &apos;211&apos;
)</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Email New Leads DACH</fullName>
        <actions>
            <name>New_Lead</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <description>https://jira.orderbird.com/browse/CDS-1038</description>
        <formula>And ( 
NOT(CONTAINS(Company, &apos;www.&apos;)),
NOT(CONTAINS(Company, &apos;http://&apos;)),
NOT(CONTAINS(Company, &apos;https://&apos;)),
NOT(ISPICKVAL( LAC_Lookup__r.LeadSource__c , &quot;Outbound&quot; )),
NOT(ISPICKVAL(  PrimaryLanguage__c , &quot;French&quot; )),
NOT(BEGINS(LAC_ID__c, &apos;10&apos;)),
NOT(BEGINS(LAC_ID__c, &apos;7&apos;)),
NOT(BEGINS(LAC_ID__c, &apos;3&apos;)),
NOT(BEGINS(LAC_ID__c, &apos;5&apos;)),
LAC_ID__c != &apos;110&apos;,
LAC_ID__c != &apos;129&apos;,
LAC_ID__c != &apos;130&apos;,
LAC_ID__c != &apos;180&apos;,
LAC_ID__c != &apos;201&apos;,
LAC_ID__c != &apos;202&apos;,
LAC_ID__c != &apos;203&apos;,
LAC_ID__c != &apos;211&apos;,
Company != &apos;Test XYZ&apos;,
FirstName != &apos;Test XYZ&apos;,
IF(ISBLANK(Origin__c),TRUE,NOT(CONTAINS(Origin__c , &apos;Ebook&apos;)))
)</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Email New Leads FR</fullName>
        <actions>
            <name>New_Lead_FR</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <formula>AND (
NOT(ISPICKVAL( LAC_Lookup__r.LeadSource__c , &quot;Outbound&quot; )),
OR (
 ISPICKVAL(Region__c, &quot;France&quot;),
 ISPICKVAL( PrimaryLanguage__c , &quot;French&quot; )
),
NOT(BEGINS(LAC_ID__c, &apos;7&apos;)), 
NOT(BEGINS(LAC_ID__c, &apos;3&apos;)), 
NOT(BEGINS(LAC_ID__c, &apos;4&apos;))
)</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
