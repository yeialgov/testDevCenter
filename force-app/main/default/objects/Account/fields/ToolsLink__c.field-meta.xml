<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ToolsLink__c</fullName>
    <description>generate link on my.orderbird backend with customers email-address</description>
    <externalId>false</externalId>
    <formula>IF(!ISNULL(myOrderbirdEmail__c) &amp;&amp; myOrderbirdEmail__c != &quot;&quot;, 

HYPERLINK(&quot;https://tools.orderbird.com/admin/accounts/venue/?q=&quot; &amp; myOrderbirdEmail__c , &quot;Click here&quot;), 

&quot;&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Click here to access to my.orderbird backend with customers email address</inlineHelpText>
    <label>Tools-Link</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
