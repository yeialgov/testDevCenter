<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>49.0</apiVersion>
    <assignments>
        <description>Set Process Trigger Text &amp; Date</description>
        <name>Set_trigger_info</name>
        <label>Set trigger info</label>
        <locationX>407</locationX>
        <locationY>50</locationY>
        <assignmentItems>
            <assignToReference>$Record.Process_Trigger_Text__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>TaskOverdue</stringValue>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>$Record.Process_Trigger_Date__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>$Flow.CurrentDate</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Update_Tasks</targetReference>
        </connector>
    </assignments>
    <description>Sets trigger word for PB &quot;A - Task&quot; to initiate the expiration process from Task //CDS-3127</description>
    <interviewLabel>Trigger Task Expiration {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Trigger Task Expiration</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>FREE_FORM_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <name>Update_Tasks</name>
        <label>Update Tasks</label>
        <locationX>557</locationX>
        <locationY>50</locationY>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <start>
        <locationX>67</locationX>
        <locationY>50</locationY>
        <connector>
            <targetReference>Set_trigger_info</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>ActivityDate</field>
            <operator>GreaterThan</operator>
            <value>
                <dateValue>2019-08-07</dateValue>
            </value>
        </filters>
        <filters>
            <field>Days_overdue__c</field>
            <operator>GreaterThanOrEqualTo</operator>
            <value>
                <numberValue>3.0</numberValue>
            </value>
        </filters>
        <filters>
            <field>Opportunity__c</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </filters>
        <filters>
            <field>Outcome__c</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <filters>
            <field>OwnerId</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue>00558000001KczGAAS</stringValue>
            </value>
        </filters>
        <object>Task</object>
        <schedule>
            <frequency>Daily</frequency>
            <startDate>2019-10-15</startDate>
            <startTime>07:30:00.000Z</startTime>
        </schedule>
        <triggerType>Scheduled</triggerType>
    </start>
    <status>Obsolete</status>
</Flow>
